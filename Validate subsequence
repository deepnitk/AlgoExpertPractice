import java.util.*;

class Program {
  public static boolean isValidSubsequence(List<Integer> array, List<Integer> sequence) {
    // Write your code here.
		int i = 0;
		int j =0;
		while (i < sequence.size() && j < array.size()){
			if( sequence.get(i) == array.get(j)){
				i++;
				j++;
			}
			else
				j++;
		}
		if ( i == sequence.size())
			return true;
		return false;
  }
}
